{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:9000"
      }
    }
  },
  "TokenOptions": {
    // "SecretKey" 用于签名和验证 JWT 令牌的秘密密钥。这应该是一个复杂的字符串，确保安全性。
    // 为空默认使用随机字符串
    "SecretKey": "",
    // "Issuer" 是发行 JWT 令牌的实体。这通常是您的应用程序或组织的名称。
    // 为空默认使用随机字符串
    "Issuer": "",
    // "Audience" 指定预期的令牌接收者。这可以是特定的用户、系统或资源的标识符。
    // 为空默认使用随机字符串
    "Audience": "",
    // "ExpireMinutes" 设置 JWT 令牌的有效期，单位为分钟。此值决定了令牌在多久后过期。
    "ExpireMinutes": 30
  },
  "IsUseSwagger": true,
  "reCAPTCHA": {
    "Client": "",
    "Server": ""
  },
  "WakeUp": {
    "Password": "",
    "SubnetBroadcastAddress": "255.255.255.255",
    "MacList": [
      {
        "Name": "台式机",
        "IP": "192.168.2.3",
        "MAC": "74:56:3C:7A:6F:70"
      }
    ]
  }
}
